      "args": "--launchTarget forge_userdev_client --version MOD_DEV --assetIndex 27 --assetsDir /home/devin/.gradle/caches/forge_gradle/assets --gameDir .",
      "env": {
        "MOD_CLASSES": "${workspaceFolder}/build/sourcesSets/main",
